node {
    stage('preparation') {
        // Checkout the master branch of the Laravel framework repository
        echo "preparation-1"
    }
    stage("composer_install") {
        // Run `composer update` as a shell script
        echo 'composer install-1'
    }
    stage("phpunit") {
        // Run PHPUnit
        echo 'vendor/bin/phpunit-1'
    }
    stage("createfile"){
        sh 'echo "abc-1" > temptemp.txt-1'
    }
    stage("cleanup"){
        deleteDir()
    }
}
